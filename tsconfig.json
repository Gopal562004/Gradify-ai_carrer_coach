{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "components/theme-provider.tsx",
    "components/Header/Header.tsx",
    "app/page.tsx",
    "app/layout.tsx"
, "app/(main)/dashboard/page.jsx", "app/(main)/interview/page.jsx", "app/(main)/interview/mock/page.jsx", "app/(main)/dashboard/_component/DashboardView.jsx", "actions/user.ts", "app/(main)/ai-cover-letter/page.jsx", "app/(main)/ai-cover-letter/[id]/page.jsx"  ],
  "exclude": ["node_modules"]
}
